{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LandUse",
  "type": "object",
  "properties": {
    "created": {
      "description": "Created timestamp of this object within the Cropwise system.",
      "type": "string",
      "format": "date-time"
    },
    "modified": {
      "description": "Modified timestamp of this object within the Cropwise system.",
      "type": "string",
      "format": "date-time"
    },
    "resource_measures": {
      "type": "array",
      "items": {
        "$ref": "RepresentationValue.schema.json#"
      }
    },
    "resource_ref": {
      "type": "string",
      "format": "uuid"
    },
    "resource_type": {
      "type": "string",
      "enum": [
        "CROPZONE",
        "FIELD"
      ]
    },
    "status": {
      "$ref": "common-enums.schema.json#/definitions/StatusEnum"
    },
    "time_scopes": {
      "type": "array",
      "items": {
        "$ref": "TimeScope.schema.json#"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "resource_ref",
    "resource_type"
  ]
}